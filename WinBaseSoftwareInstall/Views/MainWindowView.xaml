<Window
    x:Class="WinBaseSoftwareInstall.Views.MainWindowView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:local="clr-namespace:WinBaseSoftwareInstall.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModel="clr-namespace:WinBaseSoftwareInstall.ViewModels"
    Title="WinBaseSoftwareInstall | made by J0nathan550 | v1.0.0.0"
    Width="640"
    Height="480"
    d:DataContext="{d:DesignInstance Type=viewModel:MainWindowViewModel}"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Grid>
        <Button Command="{Binding SaveRelayCommand}">
            <TextBlock Text="Save Fake Data..." />
        </Button>
    </Grid>
</Window>
